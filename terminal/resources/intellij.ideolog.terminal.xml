<idea-plugin package="com.intellij.ideolog.terminal">
  <dependencies>
    <plugin id="org.jetbrains.plugins.terminal"/>
  </dependencies>
  <extensions defaultExtensionNs="org.jetbrains.plugins.terminal.exp">
    <commandBlockHighlighterProvider
      implementation="com.intellij.ideolog.terminal.highlighting.TerminalCommandBlockHighlighterProvider"/>
  </extensions>
  <extensions defaultExtensionNs="com.intellij.ideolog">
    <ideologContextDetector implementation="com.intellij.ideolog.terminal.IdeologTerminalContextDetector"/>
  </extensions>
</idea-plugin>
